#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/iut45/Etudiants/o22108337/dev_avance/TP_D-veloppement_Avanc-_Louis_Lebeaupin/node_modules/.pnpm/typeorm@0.3.20_sqlite3@5.1.7/node_modules/typeorm/node_modules:/home/iut45/Etudiants/o22108337/dev_avance/TP_D-veloppement_Avanc-_Louis_Lebeaupin/node_modules/.pnpm/typeorm@0.3.20_sqlite3@5.1.7/node_modules:/home/iut45/Etudiants/o22108337/dev_avance/TP_D-veloppement_Avanc-_Louis_Lebeaupin/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/iut45/Etudiants/o22108337/dev_avance/TP_D-veloppement_Avanc-_Louis_Lebeaupin/node_modules/.pnpm/typeorm@0.3.20_sqlite3@5.1.7/node_modules/typeorm/node_modules:/home/iut45/Etudiants/o22108337/dev_avance/TP_D-veloppement_Avanc-_Louis_Lebeaupin/node_modules/.pnpm/typeorm@0.3.20_sqlite3@5.1.7/node_modules:/home/iut45/Etudiants/o22108337/dev_avance/TP_D-veloppement_Avanc-_Louis_Lebeaupin/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../typeorm@0.3.20_sqlite3@5.1.7/node_modules/typeorm/cli.js" "$@"
else
  exec node  "$basedir/../../../../../../typeorm@0.3.20_sqlite3@5.1.7/node_modules/typeorm/cli.js" "$@"
fi
